<Application x:Class="Crypter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Crypter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Futura IGC">pack://application:,,,/Fonts/#Futura IGC</FontFamily>
        <FontFamily x:Key="Quicksand Book">pack://application:,,,/Fonts/#Quicksand Book</FontFamily>
        <FontFamily x:Key="Quicksand Dash">pack://application:,,,/Fonts/#Quicksand Dash</FontFamily>
    </Application.Resources>
</Application>
